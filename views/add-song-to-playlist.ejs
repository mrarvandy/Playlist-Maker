<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <title>Add Song</title>
    </head>
    <body>
        <h1>Add Song</h1>
        <form method="POST">
            <label for="song">Song</label><br>
            <select name="song">
                <option>--SELECT--</option>
                <% data.forEach((el) => { %>
                    <option value='<%= el.id %>'><%= el.title %></option>
                <% }) %>
            </select><br><br>
            <input type="submit" value='Submit'>
        </form>
    </body>
</html>